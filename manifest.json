{
  "manifest_version": 3,
  "name": "GodotDocumentVersionSwitcher",
  "description": "Drop down menu to change version of the Godot documentation",
  "version": "1.0",
  "icons": {


  },
  "action": {
    "default_icon": "godoticon.png",
    "default_title": "GodotDocumentVersionSwitcher",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "storage"
  ],
  "host_permissions": [
    "https://docs.godotengine.org/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://docs.godotengine.org/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ]
}